{
  "$schema": "https://raw.githubusercontent.com/neoclide/coc.nvim/master/data/schema.json",
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "suggest.snippetsSupport": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "json",
    "markdown",
    "Markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "git.addGBlameToVirtualText": true,
  "git.urlMode": "permalink",
  "java.home": "/Library/Java/JavaVirtualMachines/openjdk.jdk/Contents/Home/"
}
